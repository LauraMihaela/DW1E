// meta un numero y comprobar si es par o impar



Proceso numParImpar
	Definir num como Entera;
	Escribir "Dame el numero: " sin saltar;
	Leer num;
	
	Si(num mod 2 = 0) Entonces
		Escribir "Numero ", num, "es PAR";
	sino
		escribir "numero" ,num, "es Impar";
		
	FinSi
	
	
	
FinProceso
