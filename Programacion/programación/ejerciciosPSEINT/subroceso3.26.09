SubProceso area <- CalculoArea ( base por Referencia,altura por Referencia )
	definir area Como Real;
	Escribir"Introdusca la base" sin Saltar;
	Leer base;
	Escribir "Introdusca la altura" sin Saltar;
	Leer altura;
	area<-(base * altura)/2;
FinSubProceso





Proceso AriaTriangulo
	Definir base, altura, resultado Como Real;
	//base<-0;altura<-0;

	resultado<-CalculoArea(base,altura);
	Escribir "El aria del triangulo de base",base, " y altura ",altura, "es de ",resultado,"unidades";
	
FinProceso
